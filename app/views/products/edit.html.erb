<% provide(:title, 'New product') %>
<h1>Edit products</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@product) do |f| %>
      <%#= render 'shared/error_messages' %>
      
      <%= render 'fields', f: f %>

      <%= f.submit "Save", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
</div>


